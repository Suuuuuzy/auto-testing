.header {
  background-color: transparent;
  width: 100%;
}
.header .navBar {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  font-size: 30rpx;
  font-weight: 600;
  text-align: center;
}
.header .navBar .right_icon {
  float: left;
  padding-left: 24rpx;
}
.header .navBar .right_icon image {
  height: 46rpx;
  line-height: 46rpx;
  width: 30rpx;
}
.mask {
  background: rgba(0, 0, 0, 0.23);
  height: 100vh;
  top: 0;
  width: 100vw;
  z-index: 998;
}
.mask,
.toastpop {
  position: absolute;
}
.toastpop {
  align-items: center;
  background: #fff;
  border-radius: 12rpx;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  height: 180rpx;
  justify-content: center;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 500rpx;
  z-index: 999;
}
.toastpop .tt {
  line-height: 100rpx;
}
.toastpop .popbtn {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  height: 50rpx;
  justify-content: space-between;
  width: 100%;
}
.toastpop .popbtn .rebtn {
  border-left: 1rpx solid #bdc4cc;
  color: #52a5f8;
}
.toastpop .popbtn .btn {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  height: 100%;
  justify-content: center;
  width: 50%;
}
.wrap {
  padding: 0 32rpx;
  width: 750rpx;
}
.wrap .titles {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  height: 180rpx;
  justify-content: center;
}
.wrap .titles .bgsize {
  font-family: Courier New, Courier, monospace;
  font-size: 32rpx;
  font-weight: 700;
  line-height: 60rpx;
}
.plate {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  padding: 0 100rpx;
}
.plate .item {
  background-color: #f3f4f7;
  border-radius: 8rpx;
  color: rgba(0, 0, 0, 0.9);
  font-size: 56rpx;
  font-weight: 700;
  height: 80rpx;
  line-height: 80rpx;
  position: relative;
  text-align: center;
  width: 70rpx;
}
.plate .item.active {
  background-color: #bbb;
}
.plate .new-energy {
  border: 2rpx dashed #03be9f;
  box-sizing: border-box;
  font-weight: 700;
}
.plate .new-energy uni-icons {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
}
.plate .point {
  color: #bdc4cc;
  font-size: 18rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.panel {
  background-color: #f5f5f5;
  bottom: 0;
  box-sizing: border-box;
  left: 0;
  position: fixed;
  transform: translateY(100%);
  transition: all 0.3s ease;
  width: 100%;
  z-index: 997;
}
.panel.show {
  transform: translate(0);
}
.panel .header {
  align-items: center;
  color: #2080f7;
  display: -webkit-flex;
  display: flex;
  font-size: 34rpx;
  height: 96rpx;
  justify-content: space-between;
  padding: 0 24rpx;
}
.panel .panelList {
  padding: 10rpx 19rpx 32rpx;
}
.panel .panelList .item {
  display: inline-block;
  height: 84rpx;
  margin-bottom: 8rpx;
  margin-right: 8rpx;
  vertical-align: top;
  width: calc((100% - 24rpx) / 3);
}
.panel .panelList .item button {
  background: #fefffe;
  border: none;
  border-radius: 6rpx;
  color: rgba(0, 0, 0, 0.9);
  font-size: 32rpx;
  font-weight: 700;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  width: 100%;
}
.panel .panelList .item button.disabled {
  background-color: rgba(254, 255, 254, 0.6);
  color: rgba(0, 0, 0, 0.23);
}
.panel .panelList .backspace {
  border-radius: 6rpx;
  color: rgba(0, 0, 0, 0.9);
  font-size: 48rpx;
  font-weight: 700;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  vertical-align: top;
}
.panel .panelList .deleteimg {
  height: 36rpx;
  width: 46rpx;
}
