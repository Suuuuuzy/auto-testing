                  <text
                    class="data-v-b4c3559c"
                    style="color: rgba(41, 43, 51, 1); font-weight: 700"
                    wx:else
                  >
                    -￥{{consume.q}}元</text
                  >
                </view>
                <text class="totaloil data-v-b4c3559c">{{consume.r}}升</text>
              </view>
            </view>
