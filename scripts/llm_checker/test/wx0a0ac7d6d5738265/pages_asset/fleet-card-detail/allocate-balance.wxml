                忘记密码
              </view>
              <view
                catchtap="{{p}}"
                class="card-btns pwbtn data-v-b4c3559c"
                wx:if="{{o}}"
              >
                设置密码
              </view>
            </view>
          </view>
          <view class="recharge-bag fsb data-v-b4c3559c">
            <view class="amn-rss fdc data-v-b4c3559c">
