            <text class="backtxt data-v-b4c3559c">返回主卡</text>
          </view>
        </view>
      </view>
      <view class="consume-list data-v-b4c3559c">
        <view class="oilcard data-v-b4c3559c">
          <text class="textline data-v-b4c3559c">消费记录</text>
        </view>
        <scroll-view
          bind:refresher-refresh="{{x}}"
          bindscrolltolower="{{w}}"
          class="data-v-b4c3559c"
          lowerThreshold="{{100}}"
          scrollY="true"
          style="max-height: 50vh"
        >
          <view
            bindtap="{{consume.s}}"
            class="list data-v-b4c3559c"
            wx:for="{{t}}"
            wx:for-item="consume"
            wx:key="t"
          >
            <view class="create-time data-v-b4c3559c" wx:if="{{consume.a}}"
              >{{consume.b}}</view
            >
            <view
              class="{{['order-id','data-v-b4c3559c',consume.f&&'order-id-bg']}}"
            >
