::-webkit-scrollbar {
  display: none;
}
.ordlist {
  background: #fff;
  border-radius: 4rpx;
  flex-direction: column;
  height: 300rpx;
  justify-content: flex-start;
  width: 686rpx;
}
.nomore,
.ordlist {
  display: -webkit-flex;
  display: flex;
}
.nomore {
  align-items: center;
  background-color: #f5f7fa;
  height: 60rpx;
  justify-content: center;
  padding: 20rpx 0;
}
.items {
  margin-bottom: 24rpx;
}
.Ltime {
  color: #b3b3b3;
  font-size: 28rpx;
  font-weight: 400;
  letter-spacing: 0rpx;
  line-height: 35.68rpx;
}
.listTop {
  background: linear-gradient(124.63deg, #f7feff, #fff);
  border-radius: 4rpx 4rpx, 0rpx, 0rpx;
  font-size: 28rpx;
  height: 84rpx;
  justify-content: space-between;
  padding: 24rpx;
}
.listTop,
.tleft {
  align-items: center;
  display: -webkit-flex;
  display: flex;
}
.tleft {
  justify-content: flex-start;
}
.gunNo {
  font-size: 32rpx;
  font-weight: 700;
  line-height: 52rpx;
  margin: 0 8rpx;
}
.gunNo,
.tname {
  color: #45474d;
  letter-spacing: 0rpx;
  text-align: left;
  vertical-align: middle;
}
.tname {
  font-size: 26rpx;
  font-weight: 400;
  line-height: 48rpx;
  margin-left: 8rpx;
}
.tname,
.tright {
  align-items: center;
  display: -webkit-flex;
  display: flex;
}
.tright {
  color: #999;
  justify-content: flex-end;
}
.lefticon {
  height: 48rpx;
  left: 32rpx;
  opacity: 1;
  width: 24rpx;
}
.Mapicon {
  height: 32rpx;
  width: 32rpx;
}
.listricon {
  height: 52rpx;
  width: 52rpx;
}
.cleft {
  border-radius: 4rpx;
  color: #fff;
  height: 100rpx;
  width: 100rpx;
}
.center,
.cleft {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
}
.center {
  background: #fff;
  border-radius: 0rpx 0rpx, 4rpx, 4rpx;
  height: 132rpx;
  opacity: 1;
  padding: 24rpx 24rpx 8rpx;
  width: 686rpx;
}
.cright {
  flex-grow: 1;
  font-size: 26rpx;
  height: 122rpx;
  padding-left: 18rpx;
  position: relative;
}
.cclum,
.cright {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
}
.cclum {
  flex-direction: column;
}
.ccr {
  align-items: end;
}
.detail {
  bottom: 0;
  display: -webkit-flex;
  display: flex;
  position: absolute;
  right: 0;
  text-align: right;
}
.listbtm {
  background: #fff;
  border-radius: 4rpx 4rpx, 0rpx, 0rpx;
  height: 84rpx;
  justify-content: flex-start;
  padding: 16rpx 24rpx;
  width: 686rpx;
}
.listbtm,
.opencard {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  opacity: 1;
}
.opencard {
  border-radius: 4rpx;
  height: 50rpx;
  justify-content: center;
  padding: 8rpx 16rpx;
}
.bl,
.opencard {
  background: #ecf2fe;
  color: #00f;
}
.huise {
  background: #f5f7fa;
  color: #999;
}
.paymony {
  flex-grow: 1;
  font-size: 28rpx;
  justify-content: space-between;
  text-align: right;
}
.blig {
  color: #000;
  font-size: 32rpx;
  font-weight: 700;
}
.textline {
  color: #a9a9a9;
  font-size: 25rpx;
  padding-right: 15rpx;
  text-decoration: line-through;
}
