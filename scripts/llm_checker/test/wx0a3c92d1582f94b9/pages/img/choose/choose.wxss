.prev-container {
  align-content: space-between;
  direction: column;
  justify-content: center;
  margin-top: 20rpx;
}
.prev-container,
.prev-part {
  display: -webkit-flex;
  display: flex;
}
.prev-part {
  direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  width: 690rpx;
}
.prev-image {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  height: 230rpx;
  justify-content: center;
  position: relative;
  width: 230rpx;
}
.del-btn {
  background-color: #eee;
  border-radius: 50rpx;
  height: 50rpx;
  position: absolute;
  right: -2rpx;
  top: -4rpx;
  width: 50rpx;
  z-index: 999;
}
.add-btn,
image {
  border-radius: 12rpx;
  height: 88%;
  position: relative;
  width: 88%;
}
.add-btn:after {
  height: 100rpx;
  width: 10rpx;
}
.add-btn::before,
.add-btn:after {
  background-color: #aaa;
  border-radius: 8rpx;
  content: "";
  left: 50%;
  position: absolute;
  top: 50%;
  transform: translate(-50%, -50%);
}
.add-btn::before {
  height: 10rpx;
  width: 100rpx;
}
.add-btn {
  background: #ebebeb;
  color: #bbb;
  font-size: 150rpx;
  line-height: 115%;
  text-align: center;
}
.sew-btn::after {
  border: none;
}
.bottom-container {
  align-items: center;
  bottom: 0;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: fixed;
  width: 100%;
}
ad {
  border-radius: 12rpx;
  box-shadow: 0 4rpx 10rpx rgba(0, 0, 0, 0.1);
  margin-bottom: 30rpx;
}
.adv {
  width: 90%;
}
.btn-view {
  background-color: #fff;
  padding-top: 10px;
  width: 100%;
}
.mode-tips {
  color: #aaa;
  font-size: 14px;
  text-align: center;
}
.mode-tag {
  background-color: #eee;
  border-radius: 14px;
  padding: 2px 6px;
}
@font-face {
  font-display: block;
  font-family: icomoon;
  font-style: normal;
  font-weight: 400;
  src: url(data:application/font-woff2;charset=utf-8;base64,)
    format("woff2");
}
[class*=" i-"],
[class^="i-"] {
  speak: none;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-family: icomoon !important;
  font-style: normal;
  font-variant: normal;
  font-weight: 400;
  line-height: 1;
  text-transform: none;
}
.blue-btn,
.pink-btn {
  border-radius: 25px;
  color: #fff;
  height: 45px;
  margin-bottom: 25px;
  width: 50%;
}
.pink-btn {
  background: #f46484;
  box-shadow: 0 4px 8px rgba(244, 100, 132, 0.3);
}
buttom::after,
buttom::before {
  display: none;
}
