.page1-inputbox {
  background-color: #fff;
  box-sizing: border-box;
  color: #9b9b9b;
  padding: 15px 10px;
  text-align: left;
}
.inputlist:last-child {
  margin-bottom: 0;
  padding-top: 0;
}
.inputlist {
  border-bottom: 1px solid #f3f3f3;
  line-height: 40px;
  margin-bottom: 15px;
  min-height: 40px;
  overflow: hidden;
  padding-bottom: 10px;
}
.short-inputname {
  display: inline-block;
  float: left;
  font-size: 14px;
  width: 35%;
}
.short-input {
  border: 1px solid #f3f3f3;
  border-radius: 5px;
  box-sizing: border-box;
  display: block;
  float: right;
  height: 40px;
  overflow: hidden;
  position: relative;
  width: 65%;
}
.input {
  color: #333;
  display: block;
  font-size: 14px;
  height: 100%;
  line-height: 14px;
  outline: 0;
  text-align: center;
  width: 100%;
}
.placeholder {
  color: #ccc;
}
.start {
  background-color: #f3f3f3;
  border-radius: 6px;
  box-sizing: border-box;
  display: block;
  font-size: 16px;
  padding: 12px 10px;
}
.normalstartbtn .start {
  background: #fabf11;
  color: #fff;
  line-height: 1.5;
  margin: 13px 0 60rpx;
  text-align: center;
}
.normalstartbtn .start::before,
.normalstartbtn .start:after {
  display: none;
}
.upload-pic-containter .upload-pic-wrapper {
  color: #ff4303;
  float: right;
  font-size: 12px;
  height: 40px;
  position: relative;
  text-align: right;
  vertical-align: middle;
  width: 65%;
}
.upload-pic-containter .upload-pic-wrapper .imgHead-item::after,
.upload-pic-containter .upload-pic-wrapper .imgHead-item::before {
  display: none;
}
.upload-pic-containter .upload-pic-wrapper .imgHead-item {
  background-color: initial;
  border-radius: 0;
  color: #ff4303;
  display: inline-block;
  margin: 0;
  padding: 0;
}
.upload-pic-containter .upload-pic-pre {
  border: 1px solid #ff4303;
  border-radius: 5px;
  box-sizing: border-box;
  display: inline-block;
  font-size: 12px;
  height: 40px;
  line-height: 40px;
  overflow: hidden;
  padding: 0 10px;
  position: relative;
}
.upload-pic-containter .upload-pic-pre-value {
  -webkit-appearance: none;
  font-size: 12px;
  height: 100%;
  left: 0;
  opacity: 0;
  outline: none;
  position: absolute;
  top: 0;
  width: 100%;
}
.select {
  appearance: none;
  border: 0;
  display: block;
  height: 38px;
  line-height: 38px;
  opacity: 0;
  outline: 0;
}
.analog-select,
.select {
  font-size: 14px;
  width: 100%;
}
.analog-select {
  color: #333;
  height: 100%;
  left: 0;
  position: absolute;
  text-align: center;
  top: 0;
}
.select-arrow {
  border-left: 3px solid transparent;
  border-right: 3px solid transparent;
  border-top: 6px solid #9b9b9b;
  display: inline-block;
  height: 0;
  margin-left: 3px;
  width: 0;
}
.upload-pic-containter .upload-pic-result {
  box-sizing: border-box;
  float: right;
  height: 40px;
  position: relative;
  top: 0;
  width: 40px;
}
.bottom-box {
  background: #fafafa;
  bottom: 0;
  padding-bottom: 16rpx;
  position: fixed;
  width: 100%;
  z-index: 999999999;
}
.bottom-box .title {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.color-box {
  margin: 20rpx;
  min-height: 500rpx;
}
.set-opacity {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  font-size: 26rpx;
  padding: 0 40rpx;
}
.set-opacity .slider {
  flex-grow: 1;
  margin-left: 5rpx;
}
.ad-ok-bg {
  background: rgba(0, 0, 0, 0.2);
  height: 100%;
  left: 0;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 666;
}
.select-color-box {
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  padding: 40rpx 30rpx;
}
.select-color-box .item {
  border-radius: 8rpx;
  box-sizing: border-box;
  height: 88rpx;
  margin: 10rpx;
  position: relative;
  width: 88rpx;
}
.select-color-box .item image {
  border-radius: 8rpx;
  height: 100%;
  width: 100%;
}
.select-color-box .item.on::after {
  border: 4rpx solid #f37271;
  border-radius: 8rpx;
  content: "";
  height: 98rpx;
  left: -13rpx;
  position: absolute;
  top: -13rpx;
  width: 98rpx;
}
.color-picker {
  height: 100%;
  left: 0;
  min-height: 600rpx;
  position: absolute;
  top: 0;
  transition: all 0.1s;
  width: 100%;
}
.color-picker.bottom {
  top: 120%;
}
.ok {
  bottom: 0;
  position: absolute;
}
.ok,
.ok2 {
  background: #fff;
  border-top: 1rpx solid #ddd;
  color: #000;
  font-size: 36rpx;
  line-height: 100rpx;
  padding-bottom: 20rpx;
  text-align: center;
  width: 100%;
  z-index: 999999;
}
.gameinfobox {
  background-color: #f0f0f0;
  padding: 20rpx;
}
.water-list .item {
  display: inline-block;
  margin-left: 20rpx;
  margin-top: 20rpx;
  padding: 10rpx;
  position: relative;
  width: 185rpx;
}
.water-list .item:nth-child(3n + 1) {
  margin-left: 0;
}
.water-list .item .bg {
  border-radius: 8rpx;
  display: block;
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 100%;
}
.water-list .item .img {
  display: block;
  height: 40rpx;
  position: relative;
  width: 100%;
}
.water-list .item .x {
  display: block;
  height: 30rpx;
  position: absolute;
  right: -10rpx;
  top: -10rpx;
  width: 30rpx;
}
.ad-ok3 {
  width: 600rpx;
}
.ad-ok3 .msg {
  font-size: 22rpx;
}
.error-btn {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
}
.error-btn button::before,
.error-btn button:after {
  display: none;
}
.error-btn button {
  background-color: #f2c66f;
  color: #fff;
  font-size: 28rpx;
  margin: 15rpx;
  width: 230rpx;
}
.water-show {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  height: 400rpx;
  justify-content: center;
  position: relative;
  width: 600rpx;
}
.water-show .bg {
  display: block;
  height: 100%;
  position: absolute;
  width: 100%;
}
.water-show .img {
  display: block;
  height: 300rpx;
  position: relative;
  width: 500rpx;
}
