page {
  background-color: #eee;
  overflow-x: hidden;
  padding-bottom: 30rpx;
  width: 100%;
}
button::before,
button:after {
  display: none;
}
.img-box {
  font-size: 0;
  line-height: 0;
  margin-bottom: 10rpx;
  overflow-x: hidden;
  position: relative;
}
.img-box,
.img-box image {
  width: 100%;
}
.img-shade {
  height: 100%;
  position: absolute;
  right: 0;
  top: 0;
}
.img-shade .img {
  border-left: 2px solid #fff;
  height: 100%;
  position: relative;
}
.img-shade image {
  height: 100%;
  position: absolute;
  right: 0;
}
.img-handle {
  background: #fff;
  border-radius: 50rpx;
  color: #000;
  height: 80rpx;
  left: 0;
  width: 80rpx;
}
.img-handle,
.img-handle image {
  position: absolute;
  top: 50%;
  transform: translate(-50%, -50%);
}
.img-handle image {
  border-left: 0;
  height: 80%;
  left: 50%;
  width: 80%;
}
.bg {
  background-color: #fff;
}
.btn-box2 {
  background: #fff;
  border-radius: 16rpx;
  margin: 0 auto 20rpx;
  width: 690rpx;
}
.btn-box2,
.btn-box2 > .btn {
  box-sizing: border-box;
  padding: 20rpx;
}
.btn-box2 > .btn {
  border: 2px dashed #dedede;
  border-radius: 16rpx;
  margin: 0 auto;
  overflow: hidden;
}
.btn-box2 .msg {
  color: #999;
  font-size: 20rpx;
  margin-top: 20rpx;
  text-align: center;
}
.btn-box2 .m {
  font-size: 24rpx;
  font-weight: 700;
  letter-spacing: 5rpx;
}
.btn-box2 .btn .image {
  margin-bottom: 10rpx;
  text-align: center;
}
.btn-box2 .btn .image image {
  height: 135rpx;
  width: 193rpx;
}
.btn-box2 .btn .video {
  border-radius: 15rpx;
  position: relative;
  width: 110rpx;
}
.btn-box2 .btn .video video {
  border-radius: 15rpx 15rpx 0 0;
  height: 110rpx;
  width: 100%;
}
.btn-box2 .btn .video .close {
  height: 36rpx;
  padding: 10rpx;
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  width: 36rpx;
}
.btn-box2 .btn .video .close image {
  height: 100%;
  width: 100%;
}
.btn-box2 .btn .input {
  font-size: 26rpx;
  width: 100%;
}
.btn-box2 .btn .btns {
  justify-content: space-between;
  margin-top: 20rpx;
}
.btn-box2 .btn .btns,
.btn-box2 .btn .btns .left {
  align-items: center;
  display: -webkit-flex;
  display: flex;
}
.btn-box2 .btn .btns .left .it {
  margin-right: 20rpx;
}
.btn-box2 .btn .btns .it {
  align-items: center;
  background-color: #f5f5f5;
  border-radius: 6rpx;
  font-size: 26rpx;
  padding: 10rpx 15rpx;
}
.btn-box2 .btn .btns .it,
.videos {
  display: -webkit-flex;
  display: flex;
}
.videos .video {
  margin-right: 15rpx;
}
.videos .video video {
  display: block;
}
.videos .video:last-child {
  margin-right: 0;
}
.btn-box2 .btn .btns .it image {
  margin-right: 8rpx;
}
.btn-box2 .btn .btns .right.it {
  color: #696969;
}
.ad-box {
  background: #fff;
  border-radius: 25rpx;
  left: 50%;
  overflow: hidden;
  padding: 10rpx 20rpx 20rpx;
  position: fixed;
  text-align: center;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 640rpx;
  z-index: 9999999999;
}
.ad-box-after {
  background: #a0e75a;
  content: "";
  display: block;
  height: 4rpx;
  left: 0;
  position: absolute;
  top: 0;
  width: 0;
}
.ad-box .msg {
  color: #555;
  font-size: 28rpx;
  line-height: 80rpx;
}
.progress {
  background-color: #999;
  height: 6rpx;
  margin-top: 0;
}
.progress2 {
  background-color: hsla(0, 0%, 78%, 0.8);
  bottom: 0rpx;
  box-sizing: border-box;
  font-size: 20rpx;
  padding: 3rpx 10rpx;
  position: absolute;
  text-align: right;
  width: 100%;
}
.more-box {
  background: #eee;
  padding: 0 30rpx 20rpx;
}
.more-box,
.more-box .item {
  display: -webkit-flex;
  display: flex;
}
.more-box .item {
  align-items: center;
  background: #fff;
  border-radius: 15rpx;
  font-size: 24rpx;
  padding: 20rpx;
  position: relative;
  width: calc(50% - 50rpx);
}
.more-box .item:nth-child(1) {
  margin-right: 20rpx;
}
.more-box .item .img {
  font-size: 0;
  width: 80rpx;
}
.more-box .item .img image {
  height: 80rpx;
  width: 80rpx;
}
.more-box .item .msg {
  line-height: 38rpx;
  margin-left: 10rpx;
  width: 440rpx;
}
.more-box .item .btn {
  background: red;
  border-radius: 40rpx;
  color: #fff;
  font-size: 26rpx;
  line-height: 54rpx;
  padding: 0;
  position: absolute;
  right: 30rpx;
  text-align: center;
  width: 106rpx;
}
.bottom-btn {
  background-color: #fff;
  border-radius: 30rpx 30rpx 0 0;
  bottom: -200%;
  left: 0;
  padding-bottom: 25rpx;
  position: fixed;
  transition: all 0.6s;
  width: 100%;
  z-index: 99999;
}
.bottom-btn .box {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  padding: 50rpx 100rpx;
}
.bottom-btn .box .item {
  line-height: 40rpx;
  text-align: center;
}
.bottom-btn .box .item image {
  display: block;
  height: 80rpx;
  margin: 0 auto 15rpx;
  width: 80rpx;
}
.bottom-btn .box .item .h1 {
  font-size: 28rpx;
}
.bottom-btn .box .item .span {
  color: #999;
  font-size: 24rpx;
}
.bottom-btn .clear {
  border-top: 8rpx solid #eee;
  line-height: 86rpx;
  text-align: center;
}
.btn-view {
  margin-top: 0;
}
.vip-select-box {
  border-radius: 10rpx;
}
.vip-select-box .title {
  font-size: 28rpx;
  font-weight: 700;
  justify-content: space-between;
  margin-top: 10rpx;
  padding: 10rpx 0;
}
.vip-select-box .title,
.vip-select-box .title .history {
  align-items: center;
  display: -webkit-flex;
  display: flex;
}
.vip-select-box .title .history {
  color: #647aef;
  font-size: 24rpx;
  font-weight: 400;
}
.vip-select-box .title .history image {
  display: block;
  height: 30rpx;
  margin-right: 10rpx;
  width: 30rpx;
}
.vip-select-list {
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 10rpx auto;
}
.vip-select-list .item {
  align-items: center;
  border: 2px solid #f1c761;
  border-radius: 8rpx;
  box-sizing: border-box;
  display: -webkit-flex;
  display: flex;
  font-size: 23rpx;
  margin-top: 15rpx;
  padding: 12rpx 20rpx;
  position: relative;
  width: 317rpx;
}
.vip-select-list .item:nth-child(1),
.vip-select-list .item:nth-child(2) {
  margin-top: 0;
}
.vip-select-list .item .img {
  height: 42rpx;
  position: relative;
  width: 42rpx;
}
.vip-select-list .item .img.on image {
  display: block;
  height: 42rpx;
  width: 50rpx;
}
.vip-select-list .item .text {
  margin-left: 20rpx;
}
.vip-select-list image {
  height: 42rpx;
  left: 0;
  position: absolute;
  top: 50%;
  transform: translate(0, -50%);
  width: 42rpx;
}
.vip-select-ok {
  background: #f5c259;
  color: #3d0601;
  display: inline-block;
  font-size: 24rpx;
  font-weight: 700;
  line-height: 80rpx;
  margin: 0;
  padding-right: 70rpx;
}
.vip-select-ok image {
  height: 28rpx;
  margin-top: -14rpx;
  position: absolute;
  right: 25rpx;
  top: 50%;
  width: 36rpx;
}
.vip-select-list .xq {
  height: 40rpx;
  left: auto;
  position: absolute;
  right: 10rpx;
  top: 12rpx;
  transform: none;
  width: 40rpx;
}
.vip-box {
  justify-content: space-between;
  margin-top: 20rpx;
  padding-right: 20rpx;
}
.vip-box,
.vip-box .share-box {
  align-items: center;
  display: -webkit-flex;
  display: flex;
}
.vip-box .share-box {
  color: #647aef;
  font-size: 24rpx;
}
.vip-box .share-box .share-msg {
  align-items: center;
  display: -webkit-flex;
  display: flex;
}
.vip-box .share-box .share-msg image {
  display: block;
  height: 30rpx;
  margin-right: 10rpx;
  width: 30rpx;
}
.ad-ok {
  width: 500rpx;
}
.ad-ok .title {
  font-size: 30rpx;
  font-weight: 700;
  margin-bottom: 15rpx;
}
.ad-ok .msg {
  line-height: 42rpx;
}
.history-btn {
  display: -webkit-flex;
  display: flex;
  font-size: 28rpx;
  justify-content: space-between;
  padding: 10rpx 40rpx 30rpx;
}
.history-btn .it {
  border: 1px solid #dedede;
  border-radius: 10rpx;
  line-height: 65rpx;
  text-align: center;
  width: 160rpx;
}
.history-btn .it.right {
  background-color: #eec36a;
  border: 1px solid #eec36a;
  color: #fff;
}
.links .item {
  align-items: center;
  color: #696969;
  display: -webkit-flex;
  display: flex;
  font-size: 24rpx;
  padding: 10rpx 0;
  text-align: left;
}
.links .item .k {
  background-color: #dedede;
  border-radius: 40rpx;
  margin-right: 20rpx;
  text-align: center;
  width: 30rpx;
}
.links .item .link {
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.links .btn {
  background-color: #dedede;
  border-radius: 60rpx;
  height: 35rpx;
  margin-left: 30rpx;
  width: 35rpx;
}
.other {
  background: #fff;
  border-radius: 16rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  margin: 0 auto 30rpx;
  padding: 20rpx;
  width: 690rpx;
}
.other .h1 {
  color: #000;
  font-weight: 700;
  margin-bottom: 10rpx;
}
.other .item {
  align-items: center;
  background: initial;
  border-bottom: 1px solid #eee;
  display: -webkit-flex;
  display: flex;
  font-size: 26rpx;
  justify-content: space-between;
  line-height: 100rpx;
  padding: 0 10rpx;
  position: relative;
  text-align: left;
}
.other .item:last-child {
  border: 0;
}
.other .item .img {
  height: 40rpx;
  width: 40rpx;
}
.water-list .item {
  display: inline-block;
  margin-left: 20rpx;
  margin-top: 20rpx;
  padding: 10rpx;
  position: relative;
  width: 185rpx;
}
.water-list .item:nth-child(3n + 1) {
  margin-left: 0;
}
.water-list .item .bg {
  border-radius: 8rpx;
  display: block;
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 100%;
}
.water-list .item .img {
  display: block;
  height: 40rpx;
  position: relative;
  width: 100%;
}
.water-list .item .x {
  display: block;
  height: 30rpx;
  position: absolute;
  right: -10rpx;
  top: -10rpx;
  width: 30rpx;
}
.huakuang-box {
  display: -webkit-flex;
  display: flex;
  margin-bottom: 40rpx;
  margin-top: 20rpx;
}
.huakuang-box .it {
  border-radius: 10rpx;
  margin-left: 20rpx;
  position: relative;
  width: 150rpx;
}
.huakuang-box .it:nth-child(1) {
  margin-left: 0;
}
.huakuang-box .it .img {
  border-radius: 10rpx;
  display: block;
  height: 270rpx;
  width: 100%;
}
.huakuang-box .it .x {
  display: block;
  height: 30rpx;
  position: absolute;
  right: -10rpx;
  top: -10rpx;
  width: 30rpx;
}
.huakuang-box .it.more {
  align-items: center;
  background-color: #eac771;
  color: #fff;
  display: -webkit-flex;
  display: flex;
