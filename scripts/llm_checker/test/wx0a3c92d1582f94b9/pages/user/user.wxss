page {
  background: #f7f7f9;
}
.user-vip {
  background: #fff;
  color: #f5c259;
  padding: 30rpx 40rpx;
  position: relative;
}
.user-vip .h1 {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  font-size: 30rpx;
  font-size: 60rpx;
  font-weight: 700;
}
.user-vip .h1 image {
  height: 60rpx;
  margin-right: 10rpx;
  width: 60rpx;
}
.user-vip .p {
  color: #666;
  font-size: 26rpx;
  margin-top: 10rpx;
}
.btn:hover {
  opacity: 0.8;
}
.user-vip .btn {
  background: #f5c259;
  border-radius: 8rpx;
  color: #fff;
  font-size: 24rpx;
  padding: 10rpx 20rpx;
  position: absolute;
  right: 40rpx;
  top: 50%;
  transform: translate(0, -50%);
}
.task-box {
  background-color: #fff;
  font-size: 30rpx;
  margin-top: 30rpx;
  padding: 30rpx 40rpx;
}
.task-box .title text {
  color: #e13c49;
}
.sign-box .item-box {
  display: -webkit-flex;
  display: flex;
  font-size: 24rpx;
  justify-content: space-between;
  text-align: center;
}
.sign-box .item-box .img {
  align-items: flex-end;
  display: -webkit-flex;
  display: flex;
  height: 100rpx;
  justify-content: center;
  margin-top: 10rpx;
  position: relative;
}
.sign-box .item-box .img.hb image {
  height: 102rpx;
}
.sign-box .item-box .img.hb .multiple {
  color: #fff;
  font-size: 20rpx;
  position: absolute;
  top: 0;
  width: 100%;
}
.sign-box .item-box .msg {
  font-size: 24rpx;
  margin-top: 10rpx;
}
.sign-box .item-box image {
  height: 62rpx;
  width: 62rpx;
}
.task-box .btn {
  background-color: #e84149;
  border-radius: 50rpx;
  color: #fff;
  line-height: 80rpx;
  margin: 30rpx auto 0;
  text-align: center;
  width: 240rpx;
}
.task-box .btn.off {
  background-color: #ddd;
}
.task-box .ad .img {
  height: 62rpx;
  margin-top: 30rpx;
}
.task-box .ad .img::after {
  background-color: #f7f7f7;
  content: "";
  height: 12rpx;
  left: 50%;
  margin-top: -6rpx;
  position: absolute;
  top: 50%;
  width: 300rpx;
}
.task-box .ad .i:last-child .img::after {
  display: none;
}
.task-box .ad .img image {
  position: relative;
  z-index: 88;
}
.task-box .ad .item-box .i {
  width: 120rpx;
}
.other {
  background: #fff;
  font-size: 30rpx;
  margin-top: 30rpx;
  padding: 20rpx 40rpx;
}
.other .h1 {
  color: #000;
  font-weight: 700;
}
.other .item {
  background: initial;
  border-bottom: 1px solid #eee;
  display: block;
  font-size: 28rpx;
  line-height: 120rpx;
  padding-left: 60rpx;
  position: relative;
  text-align: left;
}
.other button::after,
.other button::before {
  display: none;
}
.other button:last-child {
  border: 0;
}
.other .icon {
  height: 35rpx;
  left: 15rpx;
  position: absolute;
  top: 50%;
  transform: translate(0, -50%);
  width: 35rpx;
}
.other .icon image {
  display: block;
  height: 100%;
  width: 100%;
}
.other button:nth-child(4) .icon {
  height: 40rpx;
  left: 12rpx;
  width: 40rpx;
}
.other button:nth-child(5) .icon {
  height: 30rpx;
  left: 20rpx;
  width: 30rpx;
}
#content {
  background: initial;
  border: 0;
  font-size: 26rpx;
  margin-bottom: 150rpx;
  margin-top: 20rpx;
}
#content text {
  color: #f5c259;
}
#content::before,
#content:after {
  display: none;
}
.invest-bg {
  background: rgba(0, 0, 0, 0.6);
  height: 100%;
  left: 0;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 99999999;
}
.invest-box {
  background: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  left: 50%;
  padding: 30rpx;
  position: relative;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 640rpx;
}
.invest-box .close {
  height: 60rpx;
  position: absolute;
  right: 0;
  top: -90rpx;
  width: 60rpx;
}
.invest-box .close image {
  display: block;
  height: 100%;
  width: 100%;
}
.invest-box .h1 {
  color: #e69c2f;
  font-size: 36rpx;
  font-weight: 700;
  padding-top: 20rpx;
  text-align: center;
}
.invest-box .top {
  height: 160rpx;
  left: 50%;
  margin-left: -70rpx;
  position: absolute;
  top: -80rpx;
  width: 140rpx;
}
.invest-box .h1 image {
  height: 80rpx;
  width: 50%;
}
.invest-box .p {
  color: #999;
  font-size: 24rpx;
  font-weight: 700;
  line-height: 60rpx;
  text-align: center;
}
.invest-box .invest-money {
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 20rpx;
}
.invest-money .li {
  border: 1px solid #c7c7c7;
  border-radius: 20rpx;
  box-sizing: border-box;
  margin-bottom: 30rpx;
  min-height: 160rpx;
  padding: 30rpx 0 0;
  position: relative;
  text-align: center;
  width: 48%;
}
.invest-money .li .bottom {
  background-color: #ff4e00;
  border-radius: 0 0 20rpx 20rpx;
  color: #fff;
  font-size: 20rpx;
  line-height: 35rpx;
}
.invest-money .li.on {
  background-color: #f6dfcf;
  border: 1px solid #df3c34;
}
.invest-money .li .typename {
  background-color: #f08a58;
  border-radius: 0 0 20rpx 0;
  bottom: 0;
  color: #fff;
  font-size: 22rpx;
  line-height: 35rpx;
  padding: 0 10rpx;
  position: absolute;
  right: 0;
}
.invest-box .msg2 {
  color: #a2a2a3;
  font-size: 22rpx;
  line-height: 30rpx;
  text-align: center;
}
.invest-money .li.on .date,
.invest-money .li.on .date .m,
.invest-money .li.on .money {
  color: #ff4e00;
}
.invest-money .li .title {
  background-color: #fff;
  border: 1px solid #df3c34;
  border-radius: 30rpx 30rpx 30rpx 0;
  box-sizing: border-box;
  color: #df3c34;
  font-size: 20rpx;
  left: -1px;
  line-height: 30rpx;
  line-height: 40rpx;
  position: absolute;
  top: -16rpx;
  width: 55%;
}
.invest-money .li .title image {
  height: 45rpx;
  left: 0;
  position: absolute;
  top: -8rpx;
}
.invest-money .li .money {
  color: #666;
  font-size: 26rpx;
  font-weight: 700;
  line-height: 40rpx;
  margin-top: 5rpx;
}
.invest-money .li.hot .title {
  padding-left: 44rpx;
  text-align: left;
}
.invest-money .li.hot .title,
.invest-money .li.hot2 .title {
  background: linear-gradient(190deg, #ff4e00, #ff9b64);
  border: 0;
  color: #fff;
  line-height: 44rpx;
}
.invest-money .li .money text {
  font-size: 28rpx;
}
.invest-money .li .date {
  align-items: flex-end;
  color: #e69c2f;
  display: -webkit-flex;
  display: flex;
  font-size: 42rpx;
  font-weight: 700;
  justify-content: center;
}
.invest-money .li .date .m {
  color: #000;
  position: relative;
}
.invest-money .li .date .s {
  bottom: 5rpx;
  color: #888;
  font-size: 24rpx;
  font-weight: 400;
  white-space: nowrap;
}
.invest-money .li .date .s text {
  display: block;
  margin-top: -8rpx;
}
.invest-money .li .hot {
  height: 80rpx;
  left: -6rpx;
  position: absolute;
  top: -6rpx;
  width: 80rpx;
}
.invest-money .li .hot image {
  height: 100%;
  width: 100%;
}
.money-ok {
  background: #ff4e00;
  border-radius: 100px;
  color: #fff;
  font-size: 30rpx;
  line-height: 70rpx;
  margin-bottom: 20rpx;
  width: 100%;
}
.money-ok::before,
.money-ok:after {
  display: none;
}
.invest-money-msg {
  color: #888;
  font-size: 34rpx;
  font-weight: 700;
  line-height: 60rpx;
  margin: 50rpx 0 80rpx;
  text-align: center;
}
.top-box {
  background: #fff;
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  margin-top: 30rpx;
  padding: 15rpx 0;
}
.top-li {
  padding: 15rpx 0;
  text-align: center;
  width: 25%;
}
.top-li image {
  height: 80rpx;
  width: 80rpx;
}
.top-li view {
  color: #333;
  font-size: 24rpx;
}
.top-li.on {
  opacity: 0.6;
}
.share-box {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
}
.share-box .left {
  font-size: 20rpx;
  line-height: 34rpx;
}
.share-box .left text {
  color: #e84149;
}
.share-box .right button {
  background-color: #e84149;
  border: 0;
  border-radius: 80rpx;
  color: #fff;
  font-size: 28rpx;
  line-height: 80rpx;
  padding: 0 40rpx;
}
.share-box .right button::after,
.share-box .rigth button::before {
  display: none;
}
.share-msg-btn {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  float: right;
  font-size: 22rpx;
}
.share-msg-btn image {
  height: 35rpx;
  width: 35rpx;
}
.sign-box.share {
  justify-content: space-between;
}
.sign-box.share,
.sign-box.share .left1 {
  display: -webkit-flex;
  display: flex;
}
.sign-box.share .left1 .im {
  height: 140rpx;
  margin-right: 40rpx;
  width: 110rpx;
}
.sign-box.share .right button {
  background-color: #e84149;
  border: 0;
  border-radius: 80rpx;
  color: #fff;
  font-size: 24rpx;
  line-height: 68rpx;
  margin-top: 70rpx;
  padding: 0 30rpx;
}
.share-box .rigth button::before,
.sign-box.share .right button::after {
  display: none;
}
.sign-box.share .title {
  font-size: 26rpx;
}
.tishi {
  align-items: center;
  background-color: #f1d8a0;
  border-radius: 10rpx;
  color: #f80a0a;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
  margin: 30rpx auto 0;
  padding: 10rpx;
  width: 700rpx;
}
.tishi .content {
  font-size: 22rpx;
}
.tishi image {
  display: block;
  height: 25rpx;
  margin-right: 10rpx;
  width: 25rpx;
}
.tishi button {
  background-color: initial;
  border: 0;
  color: #1865ff;
  font-size: 22rpx;
  line-height: normal;
  margin: 0;
  padding: 0;
}
.tishi button::after,
.tishi button::before {
  display: none;
}
.ad-ok-bg3 {
