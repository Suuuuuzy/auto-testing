.container {
  height: 100%;
  background-color: #f3f4f6;
}
#declareSuccess-top {
  position: relative;
  height: 80rpx;
  text-align: justify;
  z-index: 1;
}
#declareSuccess-top .topBg {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  width: 100%;
  height: 181rpx;
  z-index: -1;
}
#declareSuccess-top .top-cont {
  display: flex;
  height: 100%;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 28rpx;
}
.top-cont .dots-green,
.top-cont .dots-yellow {
  font-size: 18rpx;
  width: 25rpx;
  height: 25rpx;
  line-height: 25rpx;
  margin-left: 9rpx;
}
#declareSuccess-body {
  height: calc(100% - 80rpx);
  position: relative;
  margin: 0 18rpx;
  padding-left: 70rpx;
  padding-right: 54rpx;
  background-color: #fdfdfd;
  text-align: center;
  z-index: 1;
}
.declareSuccess_icon {
  width: 229rpx;
  height: 204rpx;
  margin-top: 147rpx;
  margin-left: -50rpx;
}
.declareSuccess-info {
  font-size: 45rpx;
  color: #535353;
  height: 103rpx;
  font-weight: 700;
}
.declareSuccess-detail {
  font-size: 22rpx;
  line-height: 36rpx;
  color: #a7a7a7;
}
.declareSuccess-detail .detailColor {
  color: #353535;
}
.countDown {
  font-size: 26rpx;
  margin-top: 20rpx;
}
.countDown .restTime {
  color: #67d752;
}
