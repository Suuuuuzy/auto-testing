.container {
  background-color: #f3f4f6;
}
#detailList-top {
  position: relative;
  height: 209rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
#detailList-top .detailList-top_bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 321rpx;
}
#detailList-top .top-cont {
  position: relative;
  color: #fff;
  text-align: center;
  z-index: 1;
}
.top-cont .top-title {
  font-size: 36rpx;
  line-height: 36rpx;
}
#detailList-body {
  position: relative;
  z-index: 1;
  margin: 0 18rpx;
}
.body-detail {
  background-color: #fdfdfd;
  padding: 9rpx 38rpx 19rpx;
}
.body-detail .body-detail-list {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 77rpx;
  font-size: 32rpx;
  color: #000;
}
.body-detail-list + .body-detail-list {
  border-top: 1rpx solid #f7f7f7;
}
.body-detail-list .detailList-left {
  color: #7e7e7e;
}
.body-list .body-list-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  font-size: 30rpx;
  color: #000;
  min-height: 93rpx;
  padding: 28rpx 26rpx 28rpx 35rpx;
  margin-top: 12rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.body-list-item .item-dots {
  width: 12rpx;
  height: 12rpx;
  background-color: #356ffb;
  border-radius: 50%;
  margin-right: 27rpx;
}
.body-list-item .item-name {
  padding-right: 50rpx;
  flex: 1;
}
.body-list-item .item-rightArrow {
  width: 13rpx;
  height: 23rpx;
  flex-shrink: 0;
}
