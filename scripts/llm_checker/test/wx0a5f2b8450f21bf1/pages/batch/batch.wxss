.ad {
  margin: 30rpx 20rpx 20rpx;
  width: 95%;
}
.head {
  margin-bottom: 120rpx;
}
.user-information-a {
  background-color: #fff;
  border-radius: 0rpx;
  display: -webkit-flex;
  display: flex;
  margin: 1rpx 0 0;
  padding: 0rpx 0 0;
}
.user-information_img {
  background-position: 50%;
  background-size: cover;
  border: 4rpx solid #fff;
  border-radius: 50%;
  height: 160rpx;
  margin: 30rpx;
  overflow: hidden;
  position: relative;
  width: 160rpx;
}
.avatar {
  height: 100%;
}
.nickname {
  display: -webkit-inline-flex;
  display: inline-flex;
  flex: 1;
  flex-direction: column;
  justify-content: center;
}
.user-information_nickname {
  color: #121212;
  font-size: 38rpx;
  font-weight: 700;
  line-height: 60rpx;
}
.user-txet {
  color: #7d7d7d;
  font-size: 30rpx;
  line-height: 40rpx;
  margin-top: 10rpx;
  opacity: 0.8;
}
.listvideo {
  align-items: center;
  flex-direction: column;
  height: 330rpx;
  padding: 1rpx;
  width: 249rpx;
  z-index: 1;
}
.listvideo,
.video {
  display: -webkit-flex;
  display: flex;
}
.video {
  background-color: #fff;
  flex-direction: row;
  flex-wrap: wrap;
  margin-left: 2rpx;
  width: 100%;
}
.icon {
  height: 40rpx;
  left: 9rpx;
  margin-top: 10rpx;
  position: absolute;
  width: 40rpx;
  z-index: 99;
}
.page {
  height: 100%;
  overflow-x: hidden;
}
.container {
  background: #f5f5f5;
  height: 40%;
}
.container,
.originContainer {
  box-sizing: border-box;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  padding: 20rpx;
  width: 100%;
}
.originContainer {
  background: #fff;
  border: 1px solid #e4e4e4;
  border-radius: 5px;
  height: 45%;
}
.txt {
  height: 100%;
  overflow-y: scroll;
}
.btn,
.txt {
  width: 100%;
}
.bt-a {
  float: left;
  left: 19rpx;
}
.bt-a,
.bt-b {
  margin: 0 10rpx;
  width: 46% !important;
}
.bt-b {
  float: right;
  right: 19rpx;
}
.bt-c {
  height: 70rpx;
  left: 19rpx;
  margin-top: 20rpx;
  width: 95% !important;
}
.footer {
  color: #999;
  font-size: 14px;
  margin: 20rpx 0;
  text-align: center;
}
.footer_text {
  font-size: 12px;
  padding: 0 0.34em;
}
.video_box {
  grid-column-gap: 2rpx;
  grid-row-gap: 2rpx;
  box-sizing: border-box;
  display: grid;
  grid-auto-flow: row;
  grid-template-columns: repeat(3, 1fr);
  grid-template-rows: repeat(3, 1fr);
  padding: 0 0rpx;
  width: 100%;
}
.video_item {
  height: 330rpx;
  position: relative;
  width: 100%;
}
.img_img {
  height: 100%;
  width: 100%;
}
.item_check {
  border: 3rpx solid #f5ecec;
  border-radius: 50%;
  height: 35rpx;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  width: 35rpx;
  z-index: 11;
}
.checked {
  background: #acd0e0;
  border: none;
  position: absolute;
}
.checked::after {
  border-color: #fff;
  border-style: solid;
  border-width: 0 2px 2px 0;
  content: "";
  height: 8px;
  left: 31%;
  position: absolute;
  top: 10%;
  transform: rotate(45deg);
  width: 4px;
}
.item_mask {
  background-color: rgba(0, 0, 0, 0.4);
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 100%;
  z-index: 9;
}
.toast {
  background: rgba(0, 0, 0, 0.8);
  border-radius: 10rpx;
  color: #fff;
  font-size: 35rpx;
  height: 200rpx;
  left: 50%;
  line-height: 200rpx;
  margin-left: -130rpx;
  margin-top: -130rpx;
  position: fixed;
  text-align: center;
  top: 50%;
  width: 260rpx;
  z-index: 100;
}
