}
.shareWrap {
  -webkit-box-pack: justify;
  background: #add8e6;
  box-sizing: border-box;
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  margin: 20rpx auto;
  padding: 12rpx 26rpx;
  width: 94%;
}
.fuIconWrap,
.shareWrap {
  -webkit-box-align: center;
  align-items: center;
}
.fuIconWrap {
  -webkit-box-pack: start;
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
