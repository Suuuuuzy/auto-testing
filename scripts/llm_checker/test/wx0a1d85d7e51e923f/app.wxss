page {
  --td-brand-color: #582c83;
  font-family: UKIJ Tuz;
}
.tap-page {
  padding-bottom: 60px;
}
.uy {
  font-family: UKIJ Tuz;
}
.uy-tom {
  font-family: UKIJ Tuz Tom;
}
.alp-font {
  font-family: Alp Kesme;
}
.ch-font {
  font-family: sans-serif;
}
.rtl {
  direction: rtl;
}
.ltr {
  direction: ltr;
}
.m-ico {
  font-size: 40rpx;
  text-align: left;
}
.view-center {
  display: -webkit-flex;
  display: flex;
  justify-content: center;
  margin: 0 auto;
}
.good-box {
  background-color: initial;
  border-radius: 50%;
  height: 40px;
  left: 50%;
  overflow: hidden;
  position: fixed;
  top: 50%;
  width: 40px;
  z-index: 99;
}
.coupon-item-bg {
  background-image: url(data:image/png;base64,);
  height: 150rpx;
  width: 700rpx;
}
.coupon-shop-item-bg {
  background-image: url(data:image/png;base64,);
  height: 150rpx;
  width: 700rpx;
}
.coupon-cash-item-bg {
  background-image: url(data:image/png;base64,);
  height: 150rpx;
  width: 700rpx;
}
.lan-uy-bg {
  background-image: url(data:image/png;base64,);
}
.lan-ch-bg,
.lan-uy-bg {
  height: 120rpx;
  width: 120rpx;
}
.lan-ch-bg {
  background-image: url(data:image/png;base64,);
}
.delete-txt {
  text-decoration: line-through;
}
.g-bg {
  background-color: #eaf9f6;
}
.g-bg,
.g-text {
  color: #1ea678;
}
.r-bg {
  background-color: #f8c2d9;
  color: #e5547c;
}
.title-d {
  font-family: UKIJ Tuz Tom;
  font-size: 28rpx;
  font-weight: 700;
}
.near-black,
.title-d {
  color: #494949;
}
