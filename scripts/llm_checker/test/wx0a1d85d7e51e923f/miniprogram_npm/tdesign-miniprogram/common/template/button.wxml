<template name="button">
  <t-button
    appParameter="{{appParameter||''}}"
    ariaLabel="{{ariaLabel||''}}"
    bind:agreeprivacyauthorization="onTplButtonTap"
    bind:contact="onTplButtonTap"
    bind:error="onTplButtonTap"
    bind:getphonenumber="onTplButtonTap"
    bind:getuserinfo="onTplButtonTap"
    bind:launchapp="onTplButtonTap"
    bind:opensetting="onTplButtonTap"
    bind:tap="onTplButtonTap"
    block="{{block||false}}"
    class="{{class||''}}"
    data-extra="{{extra}}"
    data-type="{{type}}"
    disabled="{{disabled||false}}"
    ghost="{{ghost||false}}"
    hoverClass="{{hoverClass||''}}"
    hoverStartTime="{{hoverStartTime||20}}"
    hoverStayTime="{{hoverStayTime||70}}"
    hoverStopPropagation="{{hoverStopPropagation||false}}"
    icon="{{icon||''}}"
    lang="{{lang||'en'}}"
    loading="{{loading||false}}"
    openType="{{openType||''}}"
    sendMessageImg="{{sendMessageImg||''}}"
    sendMessagePath="{{sendMessagePath||''}}"
    sendMessageTitle="{{sendMessageTitle||''}}"
    sessionFrom="{{sessionFrom||''}}"
    shape="{{shape||'rectangle'}}"
    showMessageCard="{{showMessageCard||false}}"
    size="{{size||'medium'}}"
    tClass="{{externalClass}}"
    tId="{{tId||''}}"
    theme="{{theme||'default'}}"
    variant="{{variant||'base'}}"
    >{{content}}</t-button
  >
</template>
