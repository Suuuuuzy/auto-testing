.record-icon {
  color: #20bf90;
  font-size: 70px;
}
.page_hd {
  padding: 20px 20px 5px;
}
.record-title {
  color: #888;
  font-size: 12px;
  margin-top: -15px;
}
.text_area {
  background-color: #fff;
  border-radius: 10px;
  min-height: 100px;
}
.sentence-bottom {
  bottom: 0;
  margin: 10px;
}
.result-area {
  height: 80px;
  padding: 10px;
}
.circle-box {
  margin-top: 0;
  text-align: center;
}
.circle {
  margin: auto;
}
._circle {
  margin: -100px auto auto;
}
.warning {
  color: red;
}
.score-good {
  color: #20bf90;
}
.score-normal {
  color: #ff0;
}
.barcode {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  height: 320rpx;
  justify-content: center;
}
.barnum {
  font-size: 38rpx;
  font-weight: 700;
  height: 100rpx;
  letter-spacing: 10rpx;
  line-height: 100rpx;
  text-align: center;
  white-space: nowrap;
  width: 670rpx;
}
.barcode > canvas {
  height: 200rpx;
  width: 680rpx;
}
.table-num {
  background-image: url(data:image/png;base64,);
  height: 200rpx;
  width: 200rpx;
}
