@import "/pages/shop-home/shop-search/shop-search.wxss";
.VerticalNav.nav {
  white-space: normal;
  width: 200rpx;
}
.VerticalNav.nav .cu-item {
  background-color: #fff;
  border: none;
  height: 90rpx;
  margin: 0;
  position: relative;
  text-align: center;
  width: 100%;
}
.VerticalNav.nav .cu-item.cur {
  background-color: #f1f1f1;
}
.VerticalNav.nav .cu-item.cur::after {
  background-color: currentColor;
  border-radius: 10rpx 0 0 10rpx;
  bottom: 0;
  content: "";
  height: 30rpx;
  margin: auto;
  position: absolute;
  right: 0rpx;
  top: 0;
  width: 8rpx;
}
.VerticalBox {
  display: -webkit-flex;
  display: flex;
}
.VerticalMain {
  background-color: #f1f1f1;
}
.tab-left {
  background-image: url(data:image/png;base64,);
}
.tab-left,
.tab-right {
  background-size: 100% 100%;
  height: 100rpx;
  width: 710rpx;
}
.tab-right {
  background-image: url(data:image/png;base64,);
}
image[class*="gif-"] {
  border-radius: 6rpx;
  display: block;
}
.shop-desc-img {
  height: 40rpx;
  width: 40rpx;
}
.add-icon,
.add-icon image {
  height: 55rpx;
  width: 55rpx;
}
.top-view {
  background-size: cover;
}
.page-color {
  background-color: #fff;
}
.nav-item {
  display: inline-block;
  height: 90rpx;
  line-height: 90rpx;
  margin: 0 10rpx;
  padding: 0 15rpx;
}
.block {
  background: #fff;
  border-radius: 16rpx;
  height: 750rpx;
  position: relative;
  width: 600rpx;
}
.close-btn {
  bottom: calc(-1 * (48rpx + 64rpx));
  left: 50%;
  margin-left: -32rpx;
  position: absolute;
}
.wrapper {
  display: block;
  margin: 32rpx;
}
