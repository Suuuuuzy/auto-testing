@import "/app.wxss";
@import "/resource/style/lib/icon.wxss";
.waterfall-page-wrapper {
  box-sizing: border-box;
  margin: 0 auto;
  overflow-x: hidden;
  overflow-y: scroll;
}
.product-item {
  align-items: center;
  border-radius: 10rpx;
  color: #fff;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
}
.waterfall-box {
  padding: 0 3vw !important;
}
.product-item-box {
  width: 340rpx !important;
}
.buy-bg {
  background-image: url(data:image/png;base64,);
  height: 100%;
}
.coupon_buy_bg {
  background-image: url(data:image/png;base64,);
}
.coupon_buy_bg,
.menu-bg {
  background-repeat: no-repeat;
  background-size: 100% 100%;
  height: 100%;
}
.menu-bg {
  background-image: url(data:image/png;base64,);
}
.scroll_box {
  overflow: hidden;
  padding: 10rpx;
  white-space: nowrap;
  width: 100%;
}
.item_list {
  display: inline-block;
  height: auto;
  margin-left: 15rpx;
  margin-right: 25rpx;
  width: 230rpx;
}
.item_list image {
  height: 200rpx;
  width: 230rpx;
}
.item_list__ {
  display: inline-block;
  height: auto;
  margin-left: 10rpx;
  margin-right: 10rpx;
  width: 170rpx;
}
.item_list__ image {
  height: 170rpx;
  width: 170rpx;
}
.item_list_ {
  display: inline-block;
  height: auto;
  margin-left: 15rpx;
  margin-right: 25rpx;
  width: 200rpx;
}
.item_list_ image {
  height: 170rpx;
  width: 200rpx;
}
.item_h_title {
  font-size: 26rpx;
  overflow: scroll;
}
.delete-text {
  text-decoration: line-through;
}
.swiper_item {
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.bar-border {
  border-radius: 12rpx;
}
