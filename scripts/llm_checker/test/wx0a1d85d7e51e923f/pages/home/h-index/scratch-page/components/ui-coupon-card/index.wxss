.wr-coupon {
  background-image: url(data:image/image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  display: -webkit-flex;
  display: flex;
  margin-bottom: 24rpx;
  overflow: hidden;
  position: relative;
}
.theme-weak.wr-coupon {
  background-image: url(https://cdn-we-retail.ym.tencent.com/miniapp/coupon/coupon-bg-grey2.png);
}
.wr-coupon__left {
  color: #fa4126;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  height: 180rpx;
  justify-content: center;
  overflow: hidden;
  position: relative;
  text-align: center;
  width: 200rpx;
}
.theme-weak .wr-coupon__left {
  color: #333;
}
.wr-coupon__left--value {
  font-family: DIN Alternate, cursive;
  font-size: 64rpx;
  font-weight: 700;
  line-height: 88rpx;
}
.wr-coupon__left--value-int {
  font-size: 48rpx;
  line-height: 88rpx;
}
.wr-coupon__left--value-decimal {
  font-size: 36rpx;
  line-height: 48rpx;
}
.wr-coupon__left--image {
  border-radius: 8px;
  height: 128rpx;
  margin-top: 30rpx;
  width: 128rpx;
}
.wr-coupon__left--desc,
.wr-coupon__left--unit {
  font-size: 24rpx;
  line-height: 32rpx;
}
.wr-coupon__left--desc {
  color: #fa4126;
}
.theme-weak .wr-coupon__left--desc {
  color: #333;
}
.wr-coupon__right {
  align-items: center;
  box-sizing: border-box;
  display: -webkit-flex;
  display: flex;
  flex-grow: 1;
  height: 180rpx;
  overflow: hidden;
  padding: 0 20rpx;
}
.wr-coupon__right--title {
  align-items: flex-start;
  color: #999;
  display: flex;
  flex: 1;
  flex-direction: column;
  font-size: 24rpx;
}
.wr-coupon__right--title .coupon-title {
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  color: #333;
  display: -webkit-box;
  font-size: 28rpx;
  font-weight: 700;
  line-height: 40rpx;
  max-width: 320rpx;
  overflow: hidden;
  white-space: normal;
}
.wr-coupon__right--title .coupon-time {
  margin-top: 16rpx;
}
.wr-coupon__right--title .coupon-desc {
  margin-top: 8rpx;
}
.wr-coupon__right--title .coupon-arrow {
  font-size: 22rpx;
}
.wr-coupon__right--oper {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
}
.wr-coupon__mask {
  background-color: #fff;
  height: 182rpx;
  left: 0;
  opacity: 0.5;
  position: absolute;
  top: 0;
  width: 702rpx;
}
.wr-coupon__tag {
  border: 0.5px solid #fa4126;
  box-sizing: border-box;
  color: #fa4126;
  font-size: 20rpx;
  height: 28rpx;
  line-height: 28rpx;
  opacity: 0.9;
  position: absolute;
  right: -24rpx;
  text-align: center;
  top: 8px;
  transform: rotate(45deg);
  width: 106rpx;
}
.wr-coupon__seal {
  background-size: 100% 100%;
  height: 128rpx;
  position: absolute;
  right: 0;
  top: 0;
  width: 128rpx;
}
.wr-coupon__seal.seal-useless {
  background-image: url(https://cdn-we-retail.ym.tencent.com/miniapp/coupon/seal-used.png);
}
.wr-coupon__seal.seal-disabled {
  background-image: url(https://cdn-we-retail.ym.tencent.com/miniapp/coupon/coupon-expired.png);
}
