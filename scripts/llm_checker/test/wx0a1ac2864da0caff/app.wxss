.container,
page {
  height: 100%;
}
.container {
  font-family: sans-serif;
  font-size: 16rpx;
  line-height: 20rpx;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
}
.a-top,
.pic {
  height: 100%;
  width: 100%;
}
.a-top {
  display: block;
}
.a-top,
.page {
  height: 100%;
  left: 0;
  opacity: 0;
  position: absolute;
  top: 0;
  width: 100%;
}
.page {
  background-color: #fff;
  line-height: 1.5;
  overflow: hidden;
  pointer-events: none;
  transition: opacity 0.4s ease-in;
}
.content,
.panel {
  overflow: hidden;
}
.content,
.content-more,
.panel {
  box-sizing: border-box;
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 100%;
}
.main {
  position: relative;
}
.content-more > ._div,
.content > ._div {
  position: absolute;
}
.content.relative > ._div {
  position: relative;
}
.page.active {
  z-index: 2;
}
.page.active,
.page.show {
  opacity: 1;
  pointer-events: all;
}
.page.show {
  transition: none;
}
.page.active.hide {
  opacity: 0;
  z-index: 3;
}
.page.xy {
  transform: translate3d(0, 100%, 0);
}
.page.xy.active {
  opacity: 1;
  transform: translateZ(0);
  transition: -webkit-transform 1s linear;
  transition: transform 1s linear;
  transition: transform 1s linear, -webkit-transform 1s linear;
  visibility: visible;
}
.page.xy.active.hide {
  opacity: 1;
  transform: translate3d(0, -100%, 0);
}
._img {
  display: block;
  width: 100%;
}
.clear {
  clear: both;
}
.clearfix:after {
  clear: both;
  content: "";
  display: table;
}
.audio {
  opacity: 0;
  pointer-events: none;
}
canvas {
  width: 100%;
}
button {
  background-color: initial;
  border: 0;
  outline: 0;
}
.photo {
  height: 100%;
  left: 0;
  opacity: 0;
  position: absolute;
  top: 0;
  width: 100%;
}
.input,
.photo {
  -webkit-appearance: none;
}
.input {
  background: none;
  border: 0;
  border-radius: 0;
  outline: none;
}
.by-width canvas {
  height: auto;
  width: 100%;
}
.by-height canvas {
  height: 100%;
  width: auto;
}
.container {
  width: 100%;
}
.page.page-shadow {
  background: rgba(0, 0, 0, 0.5);
}
.page.page-shadow.active {
  z-index: 500;
}
.common-scroll {
  bottom: 0;
  left: 0;
  position: absolute;
  top: 128rpx;
  width: 100%;
}
.nav-top {
  background: #fff;
  left: 0;
  min-height: 128rpx;
  opacity: 0;
  pointer-events: none;
  position: fixed;
  top: 0;
  transform: translateZ(0);
  transition: opacity 0.5s ease-in-out;
  width: 100%;
  z-index: 99;
}
.nav-top.active {
  opacity: 1;
  pointer-events: auto;
}
.nav-top-placeholder {
  height: 0;
}
.nav-top-inner {
  height: 128rpx;
  position: relative;
}
.nav-title {
  color: #25292a;
  font-size: 38rpx;
  font-weight: 500;
  line-height: 53rpx;
  margin: 0 auto;
  overflow: hidden;
  padding-top: 55rpx;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 360rpx;
}
.nav-top.transparent {
  background: transparent;
}
.transparent .nav-title {
  color: #fff;
}
.nav-top.active.no-touch,
.nav-top.no-touch {
  pointer-events: none;
}
.icon-back {
  bottom: 26rpx;
  height: 36rpx;
  left: 35rpx;
  position: absolute;
  width: 20rpx;
}
.nav-left-hover {
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 112rpx;
}
.icon-back-out {
  background: rgba(0, 0, 0, 0.2);
  border-radius: 50%;
  bottom: 13rpx;
  height: 64rpx;
  left: 16rpx;
  position: absolute;
  width: 64rpx;
}
.icon-back-out .icon-back {
  bottom: 14rpx;
  left: 19rpx;
  position: absolute;
}
.nav-title-logo {
  height: 53rpx;
  margin: 0 auto;
  width: 210rpx;
}
page::after {
  animation: shadow-preload 0.1s;
  animation-delay: 3s;
  content: "";
  left: -1000px;
  position: fixed;
  top: -1000px;
}
@-webkit-keyframes shadow-preload {
  0% {
    background-image: url(https://cdn1.dcloud.net.cn/img/shadow-grey.png);
  }
  100% {
    background-image: url(https://cdn1.dcloud.net.cn/img/shadow-grey.png);
  }
}
@keyframes shadow-preload {
  0% {
    background-image: url(https://cdn1.dcloud.net.cn/img/shadow-grey.png);
  }
  100% {
    background-image: url(https://cdn1.dcloud.net.cn/img/shadow-grey.png);
  }
}
[bind-data-custom-hidden="true"],
[data-custom-hidden="true"] {
  display: none !important;
}
