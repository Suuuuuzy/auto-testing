.mcontainer {
  flex-direction: column !important;
  font-family: PingFangSC, PingFangSC-Regular;
}
.main {
  padding: 72rpx;
}
.info {
  box-sizing: border-box;
  padding: 0 40rpx;
  width: 100%;
}
.user-info {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  padding-bottom: 206rpx;
  text-align: center;
  width: 100%;
}
.user-image {
  height: 160rpx;
  margin-bottom: 22rpx;
  width: 160rpx;
}
.account {
  padding-bottom: 16rpx;
}
.buttonA {
  display: -webkit-flex;
  display: flex;
  margin-bottom: 32rpx;
  width: 100%;
}
.box,
.buttonA {
  height: 100rpx;
}
.btn {
  background-color: #3469fc;
  border-radius: 4rpx;
  color: #fff;
  display: block;
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  font-weight: 400;
  padding: 20rpx 0;
}
.privacy {
  color: #1b263c;
  padding-left: 30rpx;
  position: relative;
  top: 50rpx;
}
.privacy .navigator {
  display: inline-block;
}
.privacy .navigator,
.subtext {
  color: #3469fc;
  font-size: 28rpx;
}
.subtext {
  padding-top: 30rpx;
  text-align: center;
}
.tip {
  color: grey;
  font-size: 24rpx;
  margin-left: -50rpx;
  margin-right: -50rpx;
  margin-top: 30rpx;
}
