.orderDetail {
  background-color: #efefef;
  height: 100%;
  padding: 10px;
}
.order-header {
  background-color: #fff;
  border-radius: 5px;
  box-shadow: 0 3px 6px 0 hsla(0, 0%, 89%, 0.5);
  margin-bottom: 10px;
  padding: 15px 11px 10px;
}
.header-text {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  margin-bottom: 5px;
}
.text1 {
  color: #666;
  font-size: 14px;
}
.text1,
.text2 {
  height: 19px;
  line-height: 19px;
}
.text2 {
  color: #3a7efd;
  font-size: 13px;
}
.text3 {
  color: #999;
  font-size: 12px;
  height: 19px;
  line-height: 19px;
}
.paying {
  background-clip: text;
  background-image: linear-gradient(146deg, #fc8c40 -5%, #fd5475 104%);
  color: transparent;
}
.order-content {
  background-color: #fff;
  border-radius: 5px;
  box-shadow: 0 3px 6px 0 hsla(0, 0%, 89%, 0.5);
  margin-bottom: 10px;
  padding: 13px 11px 14px;
}
.order-item-header {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  margin-bottom: 6px;
}
.text4 {
  font-size: 16px;
}
.text4,
.text5 {
  color: #333;
  height: 19px;
  line-height: 19px;
}
.text5 {
  font-size: 15px;
  font-weight: 600;
}
.text6 {
  font-size: 11px;
}
.text6,
.text7 {
  color: #999;
  height: 19px;
  line-height: 19px;
  margin-bottom: 5px;
}
.text7,
.text8 {
  font-size: 12px;
}
.text8 {
  color: #999;
  height: 19px;
  line-height: 19px;
}
.split-line {
  background-color: #eee;
  height: 1px;
  margin: 10px 0;
}
.order-total {
  color: #333;
  font-size: 15px;
  font-weight: 600;
  height: 19px;
  line-height: 19px;
  text-align: right;
}
.order-footer {
  background-color: #fff;
  border-radius: 5px;
  box-shadow: 0 3px 6px 0 hsla(0, 0%, 89%, 0.5);
  color: #999;
  font-size: 12px;
  padding: 15px 11px;
}
.text9 {
  margin-bottom: 10px;
  min-height: 19px;
}
.text10 {
  margin-bottom: 0;
}
