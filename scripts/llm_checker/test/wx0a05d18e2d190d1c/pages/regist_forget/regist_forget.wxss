.bg {
  height: 100%;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.login-img {
  position: relative;
  -webkit-box-pack: center;
  justify-content: center;
  top: 120rpx;
  height: 231rpx;
}
.login-from {
  width: 100%;
  position: absolute;
  top: 400rpx;
}
.input_account {
  position: relative;
  -webkit-box-pack: center;
  justify-content: center;
  margin: 0 auto;
  width: 500rpx;
  height: 90rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  align-items: center;
  line-height: 90rpx;
  border: 1rpx solid #fff;
  border-radius: 45rpx;
}
.inputText {
  -webkit-box-flex: 1;
  flex-grow: 1;
  width: auto;
  padding-left: 40rpx;
  color: #fff;
  font-size: 14px;
}
.loginBtnView {
  -webkit-box-align: center;
  align-items: center;
  margin: 0 auto;
  width: 500rpx;
  height: 90rpx;
}
.loginBtn {
  height: 90rpx;
  background-color: #1d2e30;
  border: none;
  border-radius: 35rpx;
  background-image: url(data:image/png;base64,);
}
.loginBtn,
.loginBtn_press {
  width: 100%;
  margin-top: 35px;
  color: #fff;
  background-size: 500rpx 90rpx;
  background-position: top;
  background-repeat: no-repeat;
}
.loginBtn_press {
  border: none;
  border-radius: 45rpx;
  background-image: url(data:image/png;base64,);
}
