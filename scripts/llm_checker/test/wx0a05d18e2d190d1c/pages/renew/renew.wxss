.empty_page {
  width: 100%;
  height: 100%;
  background: #fff;
  position: absolute;
  margin: 0 auto;
  z-index: 9999;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  flex-direction: column;
}
.iv_logo_style {
  margin-top: 300rpx;
  margin-left: 285rpx;
  width: 220rpx;
  height: 220rpx;
}
.tv_msg_style {
  text-align: center;
  padding: 20rpx;
  width: 350rpx;
  margin-left: 200rpx;
  color: #666;
}
.item_info_style {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  flex-direction: row;
}
.plate_no_style {
  color: #fff;
  border: 2px solid;
  padding: 0 10rpx;
  border-radius: 15rpx;
  background-color: #030303;
  margin-left: 20rpx;
}
.service_time_style {
  color: #4595e5;
  padding: 0 10rpx;
  margin-left: 20rpx;
}
.item_pay_style {
  padding: 10rpx;
  margin-top: 10rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  flex-direction: row;
  border: 2rpx solid;
  border-radius: 10rpx;
  -webkit-box-pack: justify;
  justify-content: space-between;
}
.item_color_nor {
  border-color: #7a7e83;
}
.item_color_sel {
  border-color: red;
}
.recommend_style {
  width: 50rpx;
  height: 50rpx;
  position: fixed;
  right: 24rpx;
}
