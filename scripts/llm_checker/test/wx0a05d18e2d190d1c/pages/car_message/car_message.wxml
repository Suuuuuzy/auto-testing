<view class="page">
  <empty
    bind:__l="__l"
    vueId="f7b43774-1"
    wx:if="{{data_list.length==0}}"
  ></empty>
  <scroll-view scrollY="true" style="width: 100%; height: 100%">
    <uni-list bind:__l="__l" vueId="f7b43774-2" vueSlots="{{['default']}}">
      <uni-list-item
        bind:__l="__l"
        bind:click="__e"
        data-event-opts="{{[ [ '^click',[ [ 'cilick_item',['$0'],[ [ ['data_list','',index] ] ] ] ] ] ]}}"
        data-value="{{item}}"
        note="{{item.subtitle}}"
        rightText="{{item.releaseTime}}"
        showExtraIcon="{{true}}"
        thumb="{{item.icon}}"
        title="{{item.title}}"
        vueId="{{'f7b43774-3-'+index+','+'f7b43774-2'}}"
        wx:for="{{data_list}}"
        wx:key="index"
      ></uni-list-item>
    </uni-list>
  </scroll-view>
</view>
