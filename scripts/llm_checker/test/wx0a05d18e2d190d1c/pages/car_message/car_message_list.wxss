.item_car_notice_style {
  -webkit-box-orient: horizontal;
  flex-direction: row;
  -webkit-box-pack: justify;
  justify-content: space-between;
}
.item_car_notice_style,
.item_system_notice_style {
  padding: 15rpx 30rpx;
  margin: 20rpx 30rpx 0rpx;
  border-radius: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-direction: normal;
  background-color: #fff;
}
.item_system_notice_style {
  -webkit-box-orient: vertical;
  flex-direction: column;
}
.item_style {
  background: #fff;
  height: 80rpx;
  position: relative;
  z-index: 2;
  background-color: #fff;
}
.view_x {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  flex-flow: row;
  -webkit-box-align: center;
  align-items: center;
  padding: 0;
  height: 60rpx;
}
.title_style {
  color: #666;
  font-size: 12;
}
.rcv_time_style {
  color: #666;
}
.separate_style {
  width: 100%;
  height: 2rpx;
  background-color: #e1e1e1;
  margin-top: 8rpx;
}
