<view>
  <empty
    bind:__l="__l"
    vueId="3da5de22-1"
    wx:if="{{data_list.length==0}}"
  ></empty>
  <scroll-view scrollY="true" style="width: 100%; height: 100%">
    <uni-list bind:__l="__l" vueId="3da5de22-2" vueSlots="{{['default']}}">
      <uni-list-item
        bind:__l="__l"
        bind:click="__e"
        data-event-opts="{{[ [ '^click',[ [ 'delete_item',['$0',index],[ [ ['data_list','',index] ] ] ] ] ] ]}}"
        data-value="{{item}}"
        note="{{item.loginTime+' '+item.version}}"
        rightText="{{item.releaseTime}}"
        title="{{item.modelNo}}"
        vueId="{{'3da5de22-3-'+index+','+'3da5de22-2'}}"
        wx:for="{{data_list}}"
        wx:key="index"
      ></uni-list-item>
    </uni-list>
  </scroll-view>
</view>
