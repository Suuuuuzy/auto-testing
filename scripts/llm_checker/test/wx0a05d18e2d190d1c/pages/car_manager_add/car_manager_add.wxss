.add_car_row {
  margin-top: 1rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  flex-direction: row;
  background-color: #fff;
  height: 88rpx;
  width: auto;
}
.add_car_content {
  left: 30rpx;
  line-height: 100%;
  margin-top: 30rpx;
  color: #242424;
}
.add_car_content,
.add_car_input {
  position: absolute;
  font-size: 30rpx;
  width: auto;
}
.add_car_input {
  height: 88rpx;
  margin-left: 30rpx;
  right: 30rpx;
  line-height: 88rpx;
  padding-right: 30rpx;
  text-align: right;
  color: #666;
}
.btn_confirm_style {
  margin-top: 120rpx;
  position: absolute;
  line-height: 88rpx;
  width: 80%;
  height: 88rpx;
  color: #fff;
  border-radius: 44rpx;
  margin-left: 10%;
}
.full_page {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background-color: #fff;
}
.view_hide {
  visibility: hidden;
}
.logo_style {
  width: 50rpx;
  height: 50rpx;
  margin: 20rpx;
}
.item_layout_style {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: relative;
  margin-top: 2rpx;
  border: -2rpx;
  border-bottom: 2px solid #eee;
  border-right-color: #eee;
  border-top-color: #eee;
}
