.wx-overlay {
  background-color: rgba(0, 0, 0, 0.5);
  height: 100%;
  left: 0;
  position: fixed;
  top: 0;
  width: 100%;
}
.wx-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.wx-multi-ellipsis--l2 {
  -webkit-line-clamp: 2;
}
.wx-multi-ellipsis--l2,
.wx-multi-ellipsis--l3 {
  -webkit-box-orient: vertical;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
}
.wx-multi-ellipsis--l3 {
  -webkit-line-clamp: 3;
}
.wx-clearfix:after {
  clear: both;
  content: "";
  display: table;
}
.wx-hairline,
.wx-hairline--bottom,
.wx-hairline--left,
.wx-hairline--right,
.wx-hairline--surround,
.wx-hairline--top,
.wx-hairline--top-bottom {
  position: relative;
}
.wx-hairline--bottom:after,
.wx-hairline--left:after,
.wx-hairline--right:after,
.wx-hairline--surround:after,
.wx-hairline--top-bottom:after,
.wx-hairline--top:after,
.wx-hairline:after {
  border: 0 solid #eee;
  bottom: -50%;
  box-sizing: border-box;
  content: " ";
  left: -50%;
  pointer-events: none;
  position: absolute;
  right: -50%;
  top: -50%;
  transform: scale(0.5);
  transform-origin: center;
}
.wx-hairline--top:after {
  border-top-width: 1px;
}
.wx-hairline--left:after {
  border-left-width: 1px;
}
.wx-hairline--right:after {
  border-right-width: 1px;
}
.wx-hairline--bottom:after {
  border-bottom-width: 1px;
}
.wx-hairline--top-bottom:after {
  border-width: 1px 0;
}
.wx-hairline--surround:after {
  border-width: 1px;
}
