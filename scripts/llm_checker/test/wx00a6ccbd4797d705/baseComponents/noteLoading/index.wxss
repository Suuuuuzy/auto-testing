.noteLoading {
  align-items: center;
  background-color: initial;
  display: flex;
  height: 100%;
  justify-content: center;
  left: 0;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 999;
}
.noteLoading .loadingImage {
  height: 70rpx;
  width: 70rpx;
}
.noteLoading .noteWarning {
  color: #fff;
  font-size: 28rpx;
  padding-top: 15rpx;
  white-space: pre-wrap;
}
.noteLoading .tc {
  background: rgba(0, 0, 0, 0.7);
  border-radius: 10px;
  padding: 40rpx;
}
