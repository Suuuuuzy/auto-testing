<u-popup
  bind:__l="__l"
  bind:close="__e"
  bind:input="__e"
  borderRadius="30"
  class="data-v-36f75ab4"
  closeIconColor="#fff"
  closeable="{{true}}"
  data-event-opts="{{[ [ '^close',[ ['close'] ] ],[ '^input',[ [ '__set_model',[ '','value','$event',[] ] ] ] ] ]}}"
  maskCloseAble="{{false}}"
  mode="center"
  value="{{value}}"
  vueId="6b23cb9e-1"
  vueSlots="{{['default']}}"
>
  <view class="home-popup-container data-v-36f75ab4">
    <view class="pop-top data-v-36f75ab4">提示</view>
    <view class="u-text data-v-36f75ab4">
      <text class="data-v-36f75ab4" wx:if="{{!text}}"
        >{{userInfo.sex===1?'获好友助力可升级骑士会员':'一键解锁查看VIP会员'}}</text
      >
      <text class="data-v-36f75ab4" wx:else>{{text}}</text>
    </view>
    <view class="pop-actions data-v-36f75ab4">
      <block wx:if="{{userInfo.sex===1}}">
        <view
          bindtap="__e"
          class="action data-v-36f75ab4"
          data-event-opts="{{[ [ 'tap',[ [ 'handleClick',['/pages/user/memberCenter/index'] ] ] ] ]}}"
          >升级vip会员</view
        >
        <view
          bindtap="__e"
          class="action data-v-36f75ab4"
          data-event-opts="{{[ [ 'tap',[ [ 'handleClick',['/pages/user/handShare/index'] ] ] ] ]}}"
          >免费解锁</view
        >
      </block>
      <view
        bindtap="__e"
        class="action data-v-36f75ab4"
        data-event-opts="{{[ [ 'tap',[ [ 'handleClick',['/pages/subPages/goddessAuth/index'] ] ] ] ]}}"
        wx:else
        >女神认证</view
      >
    </view>
  </view>
</u-popup>
